{
  "kind": "collectionType",
  "collectionName": "socials",
  "info": {
    "singularName": "social",
    "pluralName": "socials",
    "displayName": "Social"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "url": {
      "type": "string",
      "required": true,
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "social": {
